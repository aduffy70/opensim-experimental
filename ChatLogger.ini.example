[ChatLogger]
    enabled = true

    ;path to the folder where log files will be stored
    log_path = "./"

    ;unique tag to identify the log files from this opensim instance.  If files from
    ;multiple opensim instances are stored in the same folder, the tags must be unique.
    instance_tag = "myregion"

    ;channel to listen for module commands
    command_channel = 18

    ;traffic on this channel will be logged.  The standard chat channel (0) is probably
    ;most interesting, but by changing this setting the module could be used to log
    ;debug messages or traffic on other channels.
    log_channel = 0


